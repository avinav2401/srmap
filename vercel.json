{
    "version": 2,
    "builds": [
        {
            "src": "backend/api/index.go",
            "use": "@vercel/go"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/api/index.go"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}